@model GiftViewModel

<form asp-controller="Gifts" asp-action="Edit" method="post">
    <input type="hidden" asp-for="Id" />
    @await Html.PartialAsync("_CreateOrEdit")
    <button class = "btn mr-6">Update</button>
    <a class = "btn inline-block" asp-controller="Gifts" asp-action="Index">Cancel</a>
</form>