@model UserViewModel

<form asp-controller="Users" asp-action="Edit" method="post">
    <input type="hidden" asp-for="Id" />
    @await Html.PartialAsync("_CreateOrEdit")
    <button class = "ml-2 btn mr-6">Update</button>
    <a class = "bg-red-400 btn inline-block" asp-controller="Users" asp-action="Index">Cancel</a>
</form>